AFL_TRY_AFFINITY=1
AFL_SKIP_CPUFREQ=1
# command line:
'afl-fuzz' '-i' 'seed' '-o' 'output' '-f' './our_flatbuffer_mutator' './harness_fuzzed' '@@'
